package clarity;

public class Testing {
	
	private static int test;

	public static void main(String[] args) 
	{
		Testing ts = new Testing();
		test = 100;
		ts.setTest(test);
		ts.getTest(test);
		//Testing.test;
		System.out.println(ts.getTest(test));

	}

	public int getTest(int test1) 
	{
		this.test = test1;
		return test;
	}

	public void setTest(int test) 
	{
		this.test = test;
	}

}
